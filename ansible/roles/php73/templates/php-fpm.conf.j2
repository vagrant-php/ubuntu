[vagrant]
user = vagrant
group = vagrant

listen = {{ php.server.socket }}
listen.owner = vagrant
listen.group = vagrant

pm = dynamic
pm.max_children = 10
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 4
pm.max_requests = 400

request_terminate_timeout = {{ nginx.config.fastcgi_read_timeout }}s